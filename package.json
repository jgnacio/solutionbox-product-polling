    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "prisma": "^5.19.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "@google-cloud/functions-framework": "^3.4.2",
    "@prisma/client": "^5.19.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
